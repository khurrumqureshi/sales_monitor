extends layout

block body
  .container
        .row
            .span4.offset4.well
              h4 Choose Filter
              form#login(method='POST', action='/loginWeb', accept-charset='UTF-8')
                input#dp2.span4(type='text', name='username', placeholder='Start Date', data-date-format='mm/dd/yy')
                input#dp3.span4(type='password', name='password', placeholder='End Date', data-date-format='mm/dd/yy')
                button.btn.btn-info.btn-block(onclick='') Show


  script
        $(document).ready(function(){
          $('#dp2').datepicker();
          $('#dp3').datepicker();
        })